local Pets = {}
local Player = game:GetService("Players").LocalPlayer

local function getRandomPet(petTable)
    local petKeys = {}
    for key, _ in pairs(petTable) do
        table.insert(petKeys, key)
    end
    local randomKey = petKeys[math.random(1, #petKeys)]
    return petTable[randomKey]
end


for i,v in pairs(require(game.ReplicatedStorage.ClientModules.Core.ClientData).get_data()[game.Players.LocalPlayer.Name].inventory.pets) do
    local Olds = v.id
    if string.find(Olds, v.id) and v.properties.name == urban then
        
        table.insert(Pets, v.unique)
    
    end
end

local randomPet = getRandomPet(Pets)

local args = {
    [1] = randomPet
}
game:GetService("ReplicatedStorage"):WaitForChild("API"):WaitForChild("TradeAPI/AddItemToOffer"):FireServer(unpack(args))
wait(1)
